{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "@components/*": [
        "components/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@screens/*": [
        "screens/*"
      ],
      "@services/*": [
        "services/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@redux/*": [
        "redux/*"
      ],
      "@routes/*": [
        "routes/*"
      ],
      ".types/*": [
        "types/*"
      ],
      "@theme/*": [
        "components/theme/*"
      ],
    }
  },
}